<launch>
	<group ns="konenako">
		<node name="camera" pkg="konenako" type="node_camera.py" />
		<node name="printer" pkg="konenako" type="node_printer.py" output="screen" />
		<node name="detector_control_node" pkg="konenako" type="node_detector_control.py" />
		<param name="label_file" type="str" value="mscoco_complete_labels" />
		<param name="model_file" type="str" value="ssd_mobilenet_v1_1_metadata_1.tflite" />
		<param name="video_source" type="str" value="test.mp4" />
		<param name="combine_results" type="bool" value="True" />
	</group>
</launch>

